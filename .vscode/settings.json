{
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.stablePeek": true,
  "files.watcherExclude": {
    "**/.git/**": true,
    "**/node_modules/*/**": true,
    "**/build/**": true
  },
  "markdownlint.config": {
    "MD041": false
  },
  "spellright.language": [
    "en_GB"
  ],
  "spellright.notificationClass": "warning",
  "cSpell.ignoreWords": [
    "Ionescu",
    "Liviu",
    "MSYS2",
    "RPATH",
    "Somsubhra",
    "UCRT",
    "aarch",
    "armhf",
    "armv",
    "binutils",
    "caffeinate",
    "cleanall",
    "cleanlibs",
    "endraw",
    "ilegeul",
    "libexec",
    "libgcc",
    "libstdc",
    "liquidjs",
    "npmignore",
    "npmjs",
    "patchelf",
    "postversion",
    "ranlib",
    "readelf",
    "runpath",
    "userdel",
    "widl",
    "wksi",
    "workdir",
    "writable",
    "xattr",
    "xbbla",
    "xbbli",
    "xbbma",
    "xbbmi",
    "xpack",
    "xpacks"
  ]
}
